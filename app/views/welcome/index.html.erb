<% if current_user.nil? %>
  <%= button_to 'Log In', '/login', method: :get %>
  <br>
  <section id="create_user">
    <%= button_to 'Create a New User', '/register', method: :get %>
  </section>
<% else %>
  <%= button_to 'Log Out', '/logout', method: :delete %>
  <br>
  <center><p>Existing Users:</p></center>
    <% @users.each do |user| %>
    <center><div id="user-<%= user.id %>"
    <p><%= "#{user.email}" %></p>
  <% end %></center>
<% end %>